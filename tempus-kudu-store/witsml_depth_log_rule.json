{
  "name": "Well depth log Rule",
  "state": "SUSPENDED",
  "weight": 0,
  "pluginToken": "kafka-spark-streaming-sample",
  "filters": [
    {
      "configuration": {
        "messageTypes": [
          "POST_TELEMETRY_DEPTH"
        ]
      },
      "name": "MsgTypeFilter",
      "clazz": "org.thingsboard.server.extensions.core.filter.MsgTypeFilter"
    }
  ],
  "processor": null,
  "action": {
    "configuration": {
      "sync": true,
      "topic": "well-log-ds-data",
      "template": "#if($tags.size() > 0)\n\t[\n\t\t#foreach ($tag in $tags)\n\t\t  {\"id\": \"DEPTHLOG:$tag.key@$cs.nameWellbore@$cs.nameWell\", \"tsds\": \"$tag.ds\", \"value\": $tag.valueAsString}\n\t\t  #if($velocityCount < $tags.size())%!%#end\n\t\t#end\n\t]\n#end"
    },
    "clazz": "org.thingsboard.server.extensions.kafka.action.KafkaPluginAction",
    "name": "WellDepthLog"
  },
  "additionalInfo": null
}