CREATE TABLE kudu_witsml.depth_log (
   namewell STRING,
   namewellbore STRING,
   namelog STRING,
   mnemonic STRING,
   depthstring STRING,
   depth DOUBLE,
   value DOUBLE,
   PRIMARY KEY (namewell, namewellbore, namelog, mnemonic, depthstring)
 )
 PARTITION BY
 STORED AS KUDU ;

CREATE TABLE kudu_witsml.time_log (
   namewell STRING,
   namewellbore STRING,
   namelog STRING,
   mnemonic STRING,
   ts STRING,
   value DOUBLE,
   PRIMARY KEY (namewell, namewellbore, namelog, mnemonic, ts)
 )
 PARTITION BY
 STORED AS KUDU ;

CREATE TABLE kudu_witsml.message_log (
   namewell STRING,
   namewellbore STRING,
   typeMessage STRING,
   ts STRING,
   value STRING,
   mdvalue DOUBLE,
   mduom STRING,
   mdbitvalue DOUBLE,
   mdbituom STRING,
   PRIMARY KEY (namewell, namewellbore, typeMessage, ts)
 )
 PARTITION BY
 STORED AS KUDU ;

